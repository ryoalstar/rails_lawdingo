<% if @call_status == 'dialing' %>
<% elsif @call_status == 'connected' %>
  $('#dialing_div').hide();
  $('#connected_free_time_div').show();
  freeCallTimer.play();
<% elsif @call_status == 'billed' %>
  $('#call_status_span').html("<strong>Billing started</strong>");
  $('#free_text_span').hide();
  $('#time_span').hide();
  billingCallTimer.play();
<% else %>
<% end %>

